local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "DistritedHub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Welcome to DistritedHub!"})
local Tab = Window:MakeTab({
	Name = "Movesets",
	Icon = "rbxassetid://18434673438",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "RawPower"
})
OrionLib:MakeNotification({
	Name = "Thanks for using my Hub!",
	Content = "This Script is very cool",
	Image = "rbxassetid://12285568530",
	Time = 8
})
Tab:AddButton({
	Name = "RawPower Moveset",
	Callback = function()
loadstring(game:HttpGet("https://pastebin.com/raw/WeQFE7kF", true))()
      		print("button pressed")
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]
local Section = Tab:AddSection({
	Name = "Holy Guardian"
})

--[[
Name = <string> - The name of the section.
]]
