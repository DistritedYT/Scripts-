end)

player.Backpack.ChildAdded:Connect(function(child)
    if child.Name == "Omni Directional Punch" then
        detectAndRenameTool()
    end
end)

detectAndRenameTool()
local ToolName = "Umbral Teleport"
local function Script()
local teleportDistance = 40

-- Function to teleport the player
local function teleportPlayer(player)
    local character = player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        local rootPart = character.HumanoidRootPart

        -- Calculate the teleport destination
        local forwardDirection = rootPart.CFrame.LookVector
        local teleportPosition = rootPart.Position + (forwardDirection * teleportDistance)

        -- Teleport the player
        rootPart.CFrame = CFrame.new(teleportPosition)
    end
end

-- Example usage: Connect this to a button or an event
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Call the function (e.g., for testing purposes)
teleportPlayer(player)
end

local t = Instance.new("Tool")
t.Name = ToolName
t.RequiresHandle = false -- like holding an object if not add anything you will hold invisible thing
t.CanBeDropped = false
t.Parent = game.Players.LocalPlayer.Backpack
t.Activated:Connect(function() -- instead of Activated you can do Equipped or Unequipped
Script()
end)
