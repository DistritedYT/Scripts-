local Tool = script.Parent
local Players = game:GetService("Players")

-- Configure the animation ID for Boogie Woogie effect
local animationId = "rbxassetid://18715714749"  -- Replace with your actual animation ID

Tool.Activated:Connect(function()
    local player = Players:GetPlayerFromCharacter(Tool.Parent)
    local mouse = player:GetMouse()

    -- Detect the target player by clicking on them
    local target = mouse.Target
    if target and target.Parent:FindFirstChild("Humanoid") then
        local targetPlayer = Players:GetPlayerFromCharacter(target.Parent)
        
        if targetPlayer and targetPlayer ~= player then
            -- Get player and target character positions
            local playerChar = player.Character
            local targetChar = targetPlayer.Character

            if playerChar and targetChar and playerChar:FindFirstChild("HumanoidRootPart") and targetChar:FindFirstChild("HumanoidRootPart") then
                local playerRoot = playerChar.HumanoidRootPart
                local targetRoot = targetChar.HumanoidRootPart
                
                -- Swap positions
                local playerPosition = playerRoot.CFrame
                local targetPosition = targetRoot.CFrame
                playerRoot.CFrame = targetPosition
                targetRoot.CFrame = playerPosition
                
                -- Play animation for both players
                local playerAnim = Instance.new("Animation")
                playerAnim.AnimationId = animationId
                local playerHumanoid = playerChar:FindFirstChildOfClass("Humanoid")
                
                local targetAnim = Instance.new("Animation")
                targetAnim.AnimationId = animationId
                local targetHumanoid = targetChar:FindFirstChildOfClass("Humanoid")
                
                if playerHumanoid then
                    local playerAnimTrack = playerHumanoid:LoadAnimation(playerAnim)
                    playerAnimTrack:Play()
                end
                
                if targetHumanoid then
                    local targetAnimTrack = targetHumanoid:LoadAnimation(targetAnim)
                    targetAnimTrack:Play()
                end
            end
        end
    end
end)
-- Services
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

-- Local Player
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- GUI and color adjustment function
local function updateBarColor()
    -- Find the ScreenGui on the screen
    local screenGui = playerGui:FindFirstChild("ScreenGui")
    if not screenGui then return end

    -- Find the MagicHealth Frame
    local magicHealthFrame = screenGui:FindFirstChild("MagicHealth")
    if not magicHealthFrame then return end

    -- Find the Health Frame
    local healthFrame = magicHealthFrame:FindFirstChild("Health")
    if not healthFrame then return end

    -- Find the Bar Frame
    local barFrame = healthFrame:FindFirstChild("Bar")
    if not barFrame then return end

    -- Find the ImageLabel with ImageColor3 property inside the Bar Frame
    local imageLabel = barFrame:FindFirstChild("Bar")
    if not imageLabel or not imageLabel:IsA("ImageLabel") then return end

    -- Set the color to yellow
    imageLabel.ImageColor3 = Color3.fromRGB(135, 206, 235) -- Sky Blue
end

-- Check the GUI again when the character resets
local function onCharacterAdded(character)
    -- Update the GUI
    updateBarColor()
end

-- Check the local player's character
local function onPlayerAdded()
    local character = player.Character or player.CharacterAdded:Wait()
    onCharacterAdded(character)

    -- Check again when the character changes
    player.CharacterAdded:Connect(onCharacterAdded)
end

-- Check when the player is added
Players.PlayerAdded:Connect(onPlayerAdded)
if player then
    onPlayerAdded()
end
